{

    "manifest_version": 2,
    "name": "ytmusic-video-off.js",
    "version": "1.1",
  
    "description": "Stop video on music.youtube.com",
    
    "icons": {
        "48": "icons/ytmusic-video-off48.png"
      },
  
    "content_scripts": [
      {
        "matches": ["*://*.music.youtube.com/*"],
        "js": ["ytmusic-video-off.js"]
      }
    ]
  
  }